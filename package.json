    "eslint-plugin-prettier": "^5.1.2",
    "husky": "^8.0.3",
    "jest": "29.7.0",
    "lint-staged": "^15.2.0",
    "prettier": "^3.1.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "29.1.1",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
